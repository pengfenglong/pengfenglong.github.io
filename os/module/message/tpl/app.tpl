<article class="topNews">
	<section style="overflow: hidden;" class="newsList">
		<ul page="0" class="newsPage">	
			{{#each this}}
			<li class="newsHead">
				<a class="btn btn-primary btn-lg" data-toggle="modal" data-target="#pui-layer">

						<span class="newsTitle">{{title}}</span>
						<span class="newsTips">2015</span>
				</a>
			</li>
			{{/each}}
		</ul>
	</section>
</article>
